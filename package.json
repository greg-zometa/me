{
  "name": "me",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"astro check --watch\" \"astro dev\"",
    "start": "astro dev",
    "build": "astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "format": "prettier --write \"**/*.{astro,html,json,md,mdx,svelte,ts}\" --cache",
    "lint": "eslint \"**/*.ts\" --config .eslintrc --cache",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/rss": "2.4.3",
    "@astrojs/svelte": "3.1.0",
    "@resvg/resvg-js": "2.4.1",
    "astro": "2.9.1",
    "fuse.js": "6.6.2",
    "github-slugger": "2.0.0",
    "remark-collapse": "0.1.2",
    "remark-toc": "8.0.1",
    "satori": "0.10.1",
    "svelte": "4.1.1",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@astrojs/sitemap": "2.0.1",
    "@astrojs/tailwind": "4.0.0",
    "@divriots/jampack": "0.13.0",
    "@tailwindcss/typography": "0.5.9",
    "@typescript-eslint/parser": "6.1.0",
    "astro-eslint-parser": "0.14.0",
    "astrojs-service-worker": "1.0.0",
    "concurrently": "8.2.0",
    "eslint": "8.45.0",
    "eslint-plugin-astro": "0.27.2",
    "eslint-plugin-svelte": "2.32.4",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.0",
    "prettier-plugin-astro": "0.11.0",
    "prettier-plugin-svelte": "3.0.0",
    "prettier-plugin-tailwindcss": "0.4.1",
    "svelte-eslint-parser": "0.32.2"
  },
  "lint-staged": {
    "*.{astro,html,json,md,mdx,svelte,ts}": [
      "prettier --plugin-search-dir=. --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.6.5"
}
