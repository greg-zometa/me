{
  "name": "me",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"astro check --watch\" \"astro dev\"",
    "start": "astro dev",
    "build": "astro build && jampack ./dist",
    "preview": "astro preview",
    "sync": "astro sync",
    "astro": "astro",
    "clean": "rm -rf .astro .jampack dist node_modules",
    "format": "prettier --write \"**/*.{astro,html,json,md,mdx,svelte,ts}\" --cache",
    "lint": "eslint \"**/*.{astro,svelte,ts}\" --config .eslintrc --cache",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/rss": "3.0.0",
    "@astrojs/svelte": "4.0.0",
    "@resvg/resvg-js": "2.4.1",
    "astro": "3.0.8",
    "fuse.js": "6.6.2",
    "github-slugger": "2.0.0",
    "mdast-util-to-string": "4.0.0",
    "reading-time": "1.5.0",
    "remark-collapse": "0.1.2",
    "remark-external-links": "9.0.1",
    "remark-toc": "8.0.1",
    "satori": "0.10.4",
    "svelte": "4.2.0",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@astrojs/sitemap": "3.0.0",
    "@astrojs/tailwind": "5.0.0",
    "@divriots/jampack": "0.15.0",
    "@tailwindcss/typography": "0.5.9",
    "@typescript-eslint/parser": "6.5.0",
    "astro-eslint-parser": "0.15.0",
    "astrojs-service-worker": "1.1.0",
    "concurrently": "8.2.1",
    "eslint": "8.48.0",
    "eslint-plugin-astro": "0.29.0",
    "eslint-plugin-svelte": "2.33.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "svelte-eslint-parser": "0.33.0"
  },
  "lint-staged": {
    "*.{astro,html,json,md,mdx,svelte,ts}": [
      "prettier --plugin-search-dir=. --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@8.6.5"
}
