    "astro": "3.4.3",
    "fuse.js": "7.0.0",
    "github-slugger": "2.0.0",
    "mdast-util-to-string": "4.0.0",
    "reading-time": "1.5.0",
    "remark-collapse": "0.1.2",
    "remark-external-links": "9.0.1",
    "remark-toc": "9.0.0",
    "satori": "0.10.9",
    "svelte": "4.2.2",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@astrojs/sitemap": "3.0.3",
