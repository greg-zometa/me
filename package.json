    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.1",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "svelte-eslint-parser": "0.33.1"
